{
  "name": "vue-sticky-box",
  "version": "1.0.4",
  "description": "A simple vue component to be sticky",
  "keywords": [
    "vue",
    "vuejs",
    "sticky",
    "vue-sticky",
    "vuejs-sticky",
    "vuejs-sticky-box",
    "vuejs-sticky-sidebar",
    "vue-sticky-sidebar",
    "vue-sticky-box"
  ],
  "homepage": "https://navidkhm.github.io/vue-sticky-box/",
  "bugs": {
    "url": "https://github.com/Navidkhm/vue-sticky-box/issues"
  },
  "author": "Navid Khm <navidkhm@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Navidkhm/vue-sticky-box.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index-umd.js",
  "module": "./dist/index-es.js",
  "exports": {
    ".": {
      "types": "./dist/StickyBox.vue.d.ts",
      "import": "./dist/index-es.js",
      "require": "./dist/index-umd.js"
    },
    "./style.css": "./dist/vue-sticky-box.css"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "build-only": "vite build --config vite.config.demo.ts",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.83.1",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  }
}
